<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.dhina.myapplication.body">


    <ImageButton
        android:id="@+id/profile"
        android:layout_width="190dp"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/profile"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:id="@+id/search"
        android:layout_width="190dp"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/profile"
        app:srcCompat="@drawable/s4"
        android:layout_toRightOf="@+id/profile"
        android:layout_alignBottom="@+id/profile" />

    <ImageButton
        android:id="@+id/donor"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/d3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/profile" />

    <ImageButton
        android:id="@+id/request"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/r5"
        android:layout_below="@+id/search"
        android:layout_toRightOf="@+id/donor"
        android:layout_toEndOf="@+id/donor" />

    <Button
        android:id="@+id/log_out"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:text="Log out"
        android:background="#f28a68"
        android:textSize="22dp"
        android:textColor="#fcf8f4"
        android:layout_alignParentBottom="true"
        android:textStyle="bold"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
